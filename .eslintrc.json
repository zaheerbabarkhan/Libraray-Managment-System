{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier", "eslint-config-import"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": ["error", "tab"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"]
  }
}
